{
  "name": "dtsiki.github.io",
  "version": "0.1.0",
  "homepage": "https://dtsiki.github.io",
  "private": true,
  "scripts": {
    "replaceFilePaths": "node replacer.js",
    "dev": "next dev",
    "build": "next build && next export -o build && npm run replaceFilePaths",
    "start": "next start",
    "export": "next export -o build && npm run replaceFilePaths",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "format": "npx prettier --write **/*.{ts,tsx} && eslint --ext .ts,.tsx ./ --fix",
    "lint": "eslint --ext .ts,.tsx ./"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.1.1",
    "@fortawesome/free-brands-svg-icons": "^6.1.1",
    "@fortawesome/free-solid-svg-icons": "^6.1.1",
    "@fortawesome/react-fontawesome": "^0.1.18",
    "lint-staged": "^10.4.2",
    "nanoid": "^4.0.0",
    "next": "latest",
    "node-sass": "^7.0.1",
    "normalize-scss": "^7.0.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-syntax-highlighter": "^15.5.0",
    "react-yandex-metrika": "^2.6.0",
    "replace-in-file": "^6.3.5",
    "sass": "^1.52.3"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^12.1.0",
    "@next/eslint-plugin-next": "^12.1.0",
    "@types/node": "^17.0.19",
    "@types/react": "^17.0.39",
    "@types/react-gtm-module": "^2.0.1",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-syntax-highlighter": "^15.5.4",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "classnames": "^2.3.1",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "gh-pages": "^4.0.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.4",
    "prettier": "^2.5.1",
    "react-router-dom": "^6.2.1",
    "typescript": "^4.5.5"
  },
  "lint-staged": {
    "*.{ts,tsx}": "npm run format"
  },
  "builds": [
    {
      "src": "package.json",
      "use": "@now/next"
    },
    {
      "src": "static/css/style.css",
      "use": "@now/static"
    }
  ]
}
